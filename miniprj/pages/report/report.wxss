/* pages/report/report.wxss */
.viewMain{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.viewPicker{
  margin-top: 20rpx;
  font-weight: bold;
}
.viewTrans{
  height: 150rpx;
  width: 730rpx;
  border-left: 1px solid rgb(252, 252, 252);
  border-right: 1px solid rgb(252, 252, 252);
  border-top: 2px solid rgb(252, 252, 252);
  border-bottom: 2px solid rgb(252, 252, 252);
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.viewTransItemFirst{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.viewTransItemSecond{
  display: flex;
  flex-direction: row;
}
.imageTranIcon{
  border-width: 0;
  width: 70rpx;
}
.textTranName{
  display: flex;
  flex-direction: column;
  align-items: flex-start;
}
.textBlod{
  font-weight: bold;
}
.textGray{
  color: rgba(0, 0, 0, 0.555);   
}
.textSamllGray{
  color: rgba(0, 0, 0, 0.555);   
  font-size: 24rpx;    
}